{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "public/service-worker.js",
        "use": "@vercel/static"
      },
      {
        "src": "public/register-sw.html",
        "use": "@vercel/static"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          }
        ]
      }
    ]
  }
  