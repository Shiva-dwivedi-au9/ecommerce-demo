{
    "version": 2,
    "builds": [
      {
        "src": "public/service-worker.js",
        "use": "@vercel/static"
      },
      {
        "src": "public/register-sw.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/service-worker.js",
        "destination": "/public/service-worker.js"
      },
      {
        "source": "/register-sw.html",
        "destination": "/public/register-sw.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          }
        ]
      }
    ]
  }
  